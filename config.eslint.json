{
  "extends": ["eslint:all", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "spread": true,
      "experimentalObjectRestSpread": true,
      "restParams": true
    }
  },
  "globals": {
    "alert": true,
    "console": true,
    "document": true,
    "fetch": true,
    "Promise": true,
    "window": true
  },
  "rules": {
    "array-callback-return": 0,
    "capitalized-comments": 0,
    "class-methods-use-this": 0,
    "consistent-return": 1,
    "dot-location": 0,
    "eol-last": 0,
    "global-require": 1,
    "import/extensions": 0,
    "import/first": 1,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/prefer-default-export": 1,
    "linebreak-style": 0,
    "max-len": 0,
    "no-alert": 0,
    "no-console": 0,
    "no-multi-assign": 0,
    "no-param-reassign": 1,
    "no-restricted-syntax": 1,
    "no-unused-vars": 1,
    "padded-blocks": 0,
    "quote-props": [1, "as-needed"],
    "quotes": [1, "single", {
      "avoidEscape": true,
      "allowTemplateLiterals": true
    }]
  }
}
